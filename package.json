{
  "name": "@sigmacomputing/plugin",
  "version": "1.0.0-rc",
  "description": "Sigma Computing Plugin Client API",
  "license": "MIT",
  "module": "./src/index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/src",
    "!/src/**/__tests__/**"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/client/react/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/client/react/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist tsconfig*.tsbuildinfo",
    "build": "yarn clean && yarn tsc",
    "build:watch": "yarn build --watch",
    "format": "prettier --write 'src/**/*.{ts,tsx}' 'jest.config.ts'",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "test": "jest --ci",
    "test:watch": "yarn test --watch",
    "tsc": "ttsc --build tsconfig.build.json"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^2.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.2",
    "typescript-transform-paths": "^3.3.1"
  }
}
